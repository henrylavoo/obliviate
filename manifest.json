{
  "name" : "Obliviate",
  "version" : "1.1",
  "description" : "Automatically clears the cache on each page load, with ability to toggle on and off.",
  "permissions": [
    "browsingData",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
     "default_icon": "icon.png"
  },
  "background" : {
    "scripts" : ["obliviate.js"],
    "persistent": true
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
